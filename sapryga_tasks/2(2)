#include <stdio.h>
#include <stdlib.h>
int main()
{
    
    int n = 0;
    int** mass;
    int i = 0;
    int j = 0;
    int num = 1;
    int temp = num;
    scanf ("%d", &n);
    mass = (int **)malloc(n*n*sizeof(int*));
    for (i = 0; i < n; ++i)
    {
        mass[i] = (int *)malloc(n*sizeof(int));
        for (j = 0; j < n; ++j)
        {
            mass[i][j] = num;
            num = ++num;
        }
        num = ++temp;
        //++temp;
    }
    for (i = 0; i < n; ++i)
    {
        printf ("\n");
        for (j = 0; j < n; ++j)
        {
            printf ("%d ", mass[i][j]);
        }
    }
    
    return 0;
}
